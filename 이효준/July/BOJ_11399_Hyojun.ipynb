   ],
   "source": [
    "N = int(input())\n",
    "p = list(map(int, input().split()))\n",
    "a = sorted(p)\n",
    "res = 0\n",
    "\n",
    "for i in range(N):\n",
    "    for j in range(N-i):\n",
    "        res += a[j]\n",
    "\n",
    "print(res)"
   ]
